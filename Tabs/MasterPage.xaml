<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Tabs.MasterPage"
             Padding="0,40,0,0"
 
             Title="Personal Organiser">
    <ContentPage.Content>
        <StackLayout>
            <ListView x:Name="listView" SeparatorVisibility="None" RowHeight="50" Margin="30,0,0,0">
                <ListView.ItemTemplate>
                    <DataTemplate>

                        <TextCell Text="{Binding Title}"  TextColor="White" />

                    </DataTemplate>
                </ListView.ItemTemplate>
            </ListView>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>